@using SimplBlog;
@model SimplBlog.Models.Category>
@{
    
    Layout = null;
    
}

    @if(Model != null)
    {

        <ul>
            @foreach (var item in Model)
            {
                <li><a href="~/Categories/@item.CategoryId">@item.Name</a></li>

            }
        </ul>

    }


